{
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "indent": ["error", 2],
    "react/prop-types": "off",
    "semi": "error",
    "no-unused-vars": ["error", {
      "ignoreRestSiblings": true
    }]
  }
}